<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testNG-delta-trip_mgmt-OneWayTrip.xml">
    <!-- StartUpTest -->
    <parameter name="specificPropertiesFile" value="americanAirLinesProperties.properties"/>
    <parameter name="browserToUse" value="chrome"/> <!-- Change browser directly here -->
    <!-- DepartureAndArrivalCityPicker -->
    <parameter name="xpathOpenDepCitySelectorProp" value="xpathOpenDepCitySelector"/>
    <parameter name="depCityCodeProp" value="oneWayDepCityCode"/>
    <parameter name="xpathCityAutoDropDownDynamicXpathProp" value="xpathCityAutoDropDownDynamicXpath"/>
    <parameter name="xpathCityAutoDropDownCloseXpathProp" value="xpathCityAutoDropDownCloseXpath"/>

    <parameter name="xpathOpenArrCitySelectorProp" value="xpathOpenArrCitySelector"/>

    <parameter name="arrCityCodeProp" value="oneWayArrCityCode"/>
    <parameter name="xpathCityAutoDropDownDynamicXpathProp" value="xpathCityAutoDropDownDynamicXpath"/>
    <!-- TripSelectorTest -->
    <parameter name="xpathTripTypeOptionSelectorProp" value="xpathTripTypeOneWaySelector"/>
    <!-- DepartDatePickerTest -->
    <parameter name="xpathDepartDatePickerOpenerProp" value="xpathDepartDatePickerOpener"/>
    <parameter name="xpathDepartPathToOpenFirstBoxYearProp" value="xpathDepartPathToOpenFirstBoxYear"/>
    <parameter name="xpathDepartPathToOpenFirstBoxMonthProp" value="xpathDepartPathToOpenFirstBoxMonth"/>
    <parameter name="xpathDepartPathToOpenSecondBoxYearProp" value="xpathDepartPathToOpenSecondBoxYear"/>
    <parameter name="xpathDepartPathToOpenSecondBoxMonthProp" value="xpathDepartPathToOpenSecondBoxMonth"/>
    <parameter name="departDateYearProp" value="departDateYear"/>
    <parameter name="departDateMonthFirstLetterCapitalProp" value="departDateMonthFirstLetterCapital"/>
    <parameter name="xpathDepartPathToNextArrowProp" value="xpathDepartPathToNextArrow"/>
    <parameter name="xpathDepartNumericDatePathFirstBoxProp" value="xpathDepartNumericDatePathFirstBox"/>
    <parameter name="xpathDepartNumericDatePathSecondBoxProp" value="xpathDepartNumericDatePathSecondBox"/>
    <parameter name="departDateNumericDateProp" value="departDateNumericDate"/>

    <parameter name="xpathDepartDatePickerDoneButtonProp" value="xpathDepartDatePickerDoneButton"/>
    <!-- NoOfPaxTest -->
    <parameter name="xpathPaxSelectorProp" value="xpathPaxSelector"/>
    <parameter name="departNoOfPaxProp" value="oneWayNoOfPax"/>

    <parameter name="xpathPathToAllPaxOptionsProp" value="xpathPathToAllPaxOptions"/>
    <!-- FlightSearchButton -->
    <parameter name="xpathSearchButtonProp" value="xpathSearchButton"/>
    <!-- CheckResultsPageTest -->
    <parameter name="xpathResultsPageNoResultsFoundProp" value="xpathResultsPageNoResultsFound"/>
    <parameter name="xpathResultsPageFlightsFoundProp" value="xpathResultsPageFlightsFound"/>

    <listeners>
        <listener class-name="paxTransport.commonCodeFeatures.Listeners"/>
    </listeners>
    <test verbose="2" preserve-order="true"
          name="/Users/fida10/Programming/JavaProjects/MavenProjects/UnifiedAirLinesFrameWork/transportTestingFramework">
        <classes>
            <class name="paxTransport.airlines.deltaAirLines.onStartUp.StartUpTest"/>
            <class name="paxTransport.airlines.deltaAirLines.trip_mgmt.TripSelectorTest">
                <methods>
                    <exclude name="selectTripTypeDropDown"/>
                </methods>
            </class>
            <class name="paxTransport.airlines.deltaAirLines.trip_mgmt.DepartureAndArrivalCityPickerTest">
                <methods>
                    <exclude name="depCityNotDisplayedHandler"/>
                    <exclude name="arrCityNotDisplayedHandler"/>
                </methods>
            </class>
            <class name="paxTransport.airlines.deltaAirLines.trip_mgmt.DepartDatePickerTest">
                <methods>
                    <exclude name="clickDoneButton"/>
                </methods>
            </class>
            <class name="paxTransport.airlines.deltaAirLines.trip_mgmt.NoOfPaxTest"/>
            <class name="paxTransport.airlines.deltaAirLines.trip_mgmt.FlightSearchButtonTest"/>
            <class name="paxTransport.airlines.deltaAirLines.trip_mgmt.CheckResultsPageTest"/>
        </classes>
    </test>
</suite>